{
  "name": "redbit-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "shadcn:init": "npx shadcn@latest init -y",
    "shadcn:add-ui": "npx shadcn@latest add input button label select command popover table",
    "shadcn:setup": "npm run shadcn:init && npm run shadcn:add-ui",
    "openapi:generate": "npx @hey-api/openapi-ts -i http://127.0.0.1:8000/apidoc/openapi.json -o src/hey",
    "format": "prettier --write .",
    "dev": "vite",
    "test": "vitest run --reporter=verbose",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.78.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.528.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^20.8.3",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
